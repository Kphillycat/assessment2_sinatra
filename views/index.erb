<div class="hero-unit"><h1>All dem SpaceCats</h1></div>
<div class="row">
<% @spacecats.each do |sc| %>
	<div class="span4">
		<img src="http://placekitten.com/200/200" />
		<ul>
			<li><%= sc.name %></li>
			<li><%= sc.home_planet %></li>
			<li><%= sc.tagline %></li>
			<li><%= sc.superpower %></li>
		</ul>
	</div>
	<% end %>
</div>